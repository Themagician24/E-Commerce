<style>
    .text-center {
        text-align: center;
    }
    .text-right {
        text-align: right;
    }
    .text-left {
        text-align: left;
    }
    body {
        font-family: Roboto sans-serif;
    }
    table {
        border-collapse: collapse;
    }
    table td, table th {
        padding: 0.5rem;
        border-top: 1px solid darkgrey;
    }
</style>

<body>

    <div class="text-center">

        <img style="width: 100px;" src="data:image/jpeg;base64,">

        <h3>Facture commande NÂ°{{ order.id }} </h3>
        <table style="width: 100%; border-bottom: 1px solid grey">
            <tr>
                <td style="width: 49%;">
                    <strong>La Boutique Africaine</strong><br/>
                    Mon adresse,<br/>
                    Code Postal, Ville.France.<br/>
                    email@LaboutiqueAfricaine.com<br/>
                    +237 97 34 67 05
                </td>
                <td class="text-right">
                    {{ order.delivery|raw }}
                </td>
            </tr>
        </table>
    </div>
    <h5>Commande</h5>

    <table style="width: 100%;">
        <thead>

        <tr>

            <th scope="col" class="text-left">Produit</th>
            <th scope="col" class="text-left">QuantitÃ©</th>
            <th scope="col" class="text-left" >Prix unitaire H.T</th>
            <th scope="col" class="text-left">TVA</th>
            <th scope="col" class="text-right">Total T.T.C</th>
        </tr>
        </thead>
        <tbody>
        {% for product in order.orderDetails %}

            <tr>

                <td>{{ product.productName }}</td>
                <td>{{ product.productQuantity }}</td>
                <td>{{ product.productPrice|price }}</td>
                <td>{{ product.productTva }} %</td>
                {% set coeff = 1 + (product.productTva / 100 ) %}
                <td>{{ ((product.productPrice * coeff) * product.productQuantity ) | price}}</td>
            </tr>

        {% endfor %}
        <tr>

          <td>{{ order.carrierName }}</td>
            <td>1</td>
            <td></td>
            <td></td>
            <td class="text-right">{{ order.carrierPrice |price }}</td>
        </tr>


        </tbody>
    </table>

    <div class=" text-right btn btn-bg-dark btn-block  fst-italic font-weight-bold fs-6 ">
        <strong>Total H.T :</strong> {{ (order.totalWt - order.totalTva) |price }} <br/>
        <strong>Total TVA :</strong> {{ order.totalTva |price }} <br/>
        <strong>Total T.T.C :</strong> {{ order.totalWt |price }} <br/>


    </div>

</body>